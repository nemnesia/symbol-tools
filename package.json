{
  "name": "symbol-tools",
  "version": "1.0.0",
  "description": "Symbol blockchain tools and libraries monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:ui": "pnpm -r test:ui",
    "test:coverage": "pnpm -r test:coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rimraf node_modules",
    "dev": "pnpm -r --parallel dev"
  },
  "keywords": [
    "symbol",
    "blockchain",
    "nem",
    "catbuffer",
    "websocket",
    "openapi"
  ],
  "author": "ccHarvestasya",
  "packageManager": "pnpm@10.28.0",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@openapitools/openapi-generator-cli": "^2.27.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.0.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.0.0",
    "npm-check-updates": "^19.3.1",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1",
    "vite-plugin-singlefile-compression": "^2.0.5",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.16"
  },
  "volta": {
    "node": "20.19.6"
  }
}
