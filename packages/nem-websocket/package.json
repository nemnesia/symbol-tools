{
  "name": "@nemnesia/nem-websocket",
  "version": "0.1.0",
  "description": "NEM WebSocket Monitor",
  "keywords": [
    "nem",
    "blockchain",
    "websocket",
    "monitor",
    "nodejs"
  ],
  "author": "ccHarvestasya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemnesia/nem-symbol-websocket-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/nemnesia/nem-symbol-websocket-monitor/issues"
  },
  "homepage": "https://github.com/nemnesia/nem-symbol-websocket-monitor#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint \"{src,test,e2e}/**/*.ts\"",
    "format": "prettier --write \"{src,test,e2e}/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "publish:dryrun": "npm publish --dry-run",
    "publish:release": "npm run build && npm publish"
  },
  "dependencies": {
    "@stomp/stompjs": "^7.2.1",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "symbol-sdk": "^3.3.0",
    "terser": "^5.44.1",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "volta": {
    "node": "22.21.1",
    "yarn": "4.12.0"
  }
}
