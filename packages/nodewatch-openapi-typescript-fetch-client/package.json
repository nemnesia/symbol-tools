{
  "name": "@nemnesia/nodewatch-openapi-typescript-fetch-client",
  "version": "1.0.1",
  "description": "OpenAPI client for nodewatch-openapi-typescript-fetch-client",
  "keywords": [
    "nem",
    "symbol",
    "nodewatch",
    "openapi",
    "typescript",
    "fetch",
    "client",
    "blockchain"
  ],
  "author": "The Symbol Syndicate",
  "contributors": [
    {
      "name": "ccHarvestasya",
      "x.com": "@ccHarvestasya"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nemnesia/symbol-tools/tree/main/packages/nodewatch-openapi-fetch-client"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "gen": "rimraf src && openapi-generator-cli generate -i openapi-nodewatch.yaml -g typescript-fetch -o src -t custom-templates",
    "get": "curl -so openapi-nodewatch.yaml https://nodewatch.symbol.tools/static/openapi/openapi.yaml",
    "test": "vitest run",
    "lint": "eslint --fix \"{src,test,e2e}/**/*.ts\"",
    "format": "prettier --write \"{src,test,e2e}/**/*.ts\"",
    "publish:dryrun": "npm publish --dry-run",
    "publish:release": "npm run build && npm publish",
    "prepublish:check": "npm run lint && npm run format && npm run build && npm test",
    "custom-templates": "openapi-generator-cli author template -g typescript-fetch -o custom-templates"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "volta": {
    "node": "20.19.6"
  }
}
