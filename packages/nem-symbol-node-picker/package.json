{
  "name": "nem-symbol-node-picker",
  "version": "1.2.0",
  "description": "A lightweight Node.js package for picking random NEM and Symbol blockchain nodes from NodeWatch API",
  "keywords": [
    "nem",
    "symbol",
    "nodewatch",
    "blockchain",
    "node",
    "picker",
    "nodejs"
  ],
  "author": "ccHarvestasya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemnesia/nem-symbol-node-picker.git"
  },
  "bugs": {
    "url": "https://github.com/nemnesia/nem-symbol-node-picker/issues"
  },
  "homepage": "https://github.com/nemnesia/nem-symbol-node-picker#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsup",
    "lint": "eslint \"{src,test,e2e}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,test,e2e}/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:e2e": "vitest run --dir e2e",
    "test:e2e:watch": "vitest --watch --dir e2e",
    "test:e2e:coverage": "vitest --coverage --dir e2e",
    "test:e2e:coverage:ui": "vitest --ui --coverage --dir e2e",
    "test:all": "vitest run",
    "prepublishOnly": "pnpm run lint && pnpm run format && pnpm run build && pnpm test",
    "publish:dryrun": "npm publish --dry-run",
    "publish:release": "npm run build && npm publish"
  },
  "dependencies": {
    "@nemnesia/nodewatch-openapi-typescript-fetch-client": "^1.0.1",
    "@nemnesia/nodewatch-openapi-provider": "workspace:*"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=20.0.0"
  },
  "volta": {
    "node": "20.19.5",
    "yarn": "4.12.0"
  }
}
