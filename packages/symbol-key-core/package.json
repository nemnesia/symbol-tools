{
  "name": "@nemnesia/symbol-keyring",
  "version": "0.1.0",
  "description": "Symbolブロックチェーン用のed25519鍵生成・署名ライブラリ",
  "author": "harvestasya",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm run clean && tsup",
    "clean": "rm -rf dist",
    "lint": "eslint \"{src,test,e2e}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,test,e2e}/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "prepublishOnly": "pnpm run build && pnpm test",
    "publish:dryrun": "npm publish --dry-run",
    "publish:release": "npm run build && npm publish"
  },
  "keywords": [
    "symbol",
    "keyring",
    "wallet",
    "blockchain",
    "nem",
    "crypto"
  ],
  "packageManager": "pnpm@10.26.2",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harvestasya/symbol-tools.git",
    "directory": "packages/symbol-keyring"
  },
  "bugs": {
    "url": "https://github.com/harvestasya/symbol-tools/issues"
  },
  "homepage": "https://github.com/harvestasya/symbol-tools/tree/main/packages/symbol-keyring#readme",
  "dependencies": {
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1",
    "@scure/bip39": "^2.0.1"
  }
}
