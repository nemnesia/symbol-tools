{
  "name": "@nemnesia/symbol-catbuffer",
  "version": "0.1.0",
  "description": "Symbol blockchain catbuffer utilities and type definitions for JavaScript/TypeScript",
  "keywords": [
    "symbol",
    "blockchain",
    "catbuffer",
    "serialization",
    "nem"
  ],
  "homepage": "https://github.com/nemnesia/symbol-tools/blob/main/packages/symbol-catbuffer/README.md",
  "bugs": {
    "url": "https://github.com/nemnesia/symbol-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemnesia/symbol-tools.git",
    "directory": "packages/symbol-catbuffer"
  },
  "license": "MIT",
  "author": "The Symbol Syndicate",
  "contributors": [
    {
      "name": "ccHarvestasya",
      "x.com": "@ccHarvestasya"
    }
  ],
  "files": [
    "src",
    "types",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "main": "./src/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./types/index.d.ts",
      "default": "./src/index.js"
    },
    "./symbol": {
      "import": "./src/symbol/index.js",
      "types": "./types/symbol/index.d.ts",
      "default": "./src/symbol/index.js"
    }
  },
  "scripts": {
    "copy:models": "bash ./scripts/copy_models.sh",
    "test": "vitest run",
    "prepublishOnly": "npm test",
    "publish:dryrun": "npm publish --dry-run",
    "publish:release": "npm publish"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "20.19.6"
  },
  "dependencies": {
    "@noble/hashes": "^2.0.1"
  },
  "devDependencies": {
    "eslint-plugin-n": "^17.23.1"
  }
}
