{
  "name": "symbol-qr-library",
  "version": "0.14.3",
  "description": "Symbol library to handle QR Codes",
  "author": "Gr√©gory Saive from Using Blockchain Ltd <greg@ubc.digital>",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsup",
    "lint": "eslint \"{src,test,e2e,examples}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,test,e2e,examples}/**/*.ts\"",
    "test": "vitest run --no-ui test/**/*.test.ts",
    "test:watch": "vitest --watch test/**/*.test.ts",
    "test:coverage": "vitest run --no-ui --coverage test/**/*.test.ts",
    "test:e2e": "vitest run --no-ui e2e",
    "test:e2e:watch": "vitest --watch e2e",
    "test:all": "vitest run test/**/*.test.ts && vitest run e2e",
    "publish:dryrun": "npm publish --dry-run",
    "publish:release": "npm run build && npm publish",
    "doc": "typedoc --entryPointStrategy expand --out \"ts-docs/$(npm run version --silent)\" src",
    "create-index": "cti create ./src -b -n -e commands",
    "server": "npx http-server . -l 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemtech/symbol-qr-library.git"
  },
  "bugs": {
    "url": "https://github.com/nemtech/symbol-qr-library/issues"
  },
  "homepage": "https://github.com/nemtech/symbol-qr-library#readme",
  "dependencies": {
    "@noble/ciphers": "^2.0.1",
    "@noble/hashes": "^2.0.1"
  },
  "peerDependencies": {
    "symbol-sdk": "^3.3.0"
  },
  "volta": {
    "node": "20.19.6"
  }
}
